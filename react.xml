<Building>
<ProjectInformation></ProjectInformation>
	<Units>
		<IsMetric>bool</IsMetric>
	</Units>
	<!-- An internal coordinate system is set and not modifiable.-->
	<!-- Multiple CoordinateSystem would be allowed; this gives the ability to name different systems and shows different measurements as the designer requires-->
	<CoordinateSystem>
		<!-- Id is not nullable-->
		<Id>Guid</Id>
		<Name>"null"</Name>
	</CoordinateSystem>
	<!-- Multiple levels are available-->
	<!-- Two levels cannot contain the same elevation height or the same name-->
	<Levels>
		<Level>
			<Elevation>double</Elevation>
			<Name>string</Name>
		</Level>
	</Levels>
	<Floors>
		<Floor>
			<BoundryPoints>
				<Point>
					<X>double</X>
					<Y>double</Y>
					<Z>double</Z>
				</Point>
			</BoundryPoints>
		</Floor>
	</Floors>
	<Walls>
		<Wall>
			<StartPoint>
				<X>double</X>
				<Y>double</Y>
				<Z>double</Z>
			</StartPoint>
			<EndPoint>
				<X>double</X>
				<Y>double</Y>
				<Z>double</Z>
			</EndPoint>
			<Height>double</Height>
		</Wall>
	</Walls>
</Building>